{
  "name": "lamalama/laravel-login-attempts",
  "description": "Register unique login attempts and get notified of suspicious attempts via email",
  "keywords": [
    "laravel",
    "auth",
    "authorization",
    "login"
  ],
  "type": "library",
  "homepage": "https://github.com/lamalamaNL/laravel-login-attempts",
  "license": "MIT",
  "authors": [
    {
      "name": "Lama Lama BV",
      "email": "info@lamalama.nl"
    },
    {
      "name": "Mark de Vries",
      "email": "mark@lamalama.nl"
    }
  ],
  "require": {
    "torann/geoip": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "LamaLama\\LoginAttempts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LamaLama\\LoginAttempts\\Tests\\": "tests"
    }
  },
  "require-dev": {
  },
  "extra": {
    "laravel": {
        "providers": [
            "LamaLama\\LoginAttempts\\LoginAttemptsServiceProvider"
        ],
        "aliases": {
            "LoginAttempts": "LamaLama\\LoginAttempts\\Facades\\LoginAttempts"
        }
    }
  }
}
