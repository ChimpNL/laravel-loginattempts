{
  "name": "chimp/laravel-loginattempts",
  "description": "Register unique login attempts and get notified of suspicious attempts",
  "keywords": ["laravel", "auth", "authorization", "login"],
  "type": "library",
  "homepage": "https://github.com/ChimpNL/laravel-loginattempts",
  "license": "MIT",
  "authors": [
    {
      "name": "Chimp BV",
      "email": "info@chimp.nl"
    }
  ],
  "require": {
    "torann/geoip": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Chimp\\LaravelLoginAttempts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Chimp\\LaravelLoginAttempts\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.1",
    "orchestra/testbench": "^3.5"
  }
}
